PROFILE=your AWS credentials profile # this is required
REGION=eu-west-2 # AWS region to deploy to. Default value is eu-west-2
IS_PRODUCTION=false # this value represents whether environment is production or other. Currently this is not implemented fully as I am using only one environment. Default value is false
IS_LOCAL=false # whether it is local development environment or remote. This variable decides which S3 bucket or DynamoDB to hit
